{
    "name": "supravatm/laravel-clover-pay",
    "description": "A Laravel package to integrate Clover payment gateway with credit card checkout.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Supravatm\\CloverPayment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Supravatm\\CloverPayment\\CloverPaymentServiceProvider"
            ]
        }
    }
}